        <AbilityScoreFeature>
          <Keywords />
          <Name>Strength</Name>
          <Text>Increase your Strength by 1</Text>
          <Level>1</Level>
          <Hidden>true</Hidden>
          <NoDisplay>false</NoDisplay>
          <Action>Hidden</Action>
          <Modifier>AddScore</Modifier>
          <Strength>1</Strength>
